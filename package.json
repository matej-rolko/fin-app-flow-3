        "bcryptjs": "^3.0.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "concurrently": "^9.1.2",
        "expo-secure-store": "^14.0.1",
        "jsonwebtoken": "^9.0.2",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0"
    }
}
